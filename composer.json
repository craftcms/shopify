{
    "name": "elivz/shopify-product-fetcher",
    "description": "Load your Shopify products into your craft3 cms instance",
    "type": "craft-plugin",
    "version": "1.0.3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "shopify", 
        "fetch-products",
        "integrate shopify",
        "product sync"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "niklas",
            "homepage": "https://maier-niklas.de/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "\\shopify\\": "src/"
        }
    },
    "extra": {
        "name": "Shopify",
        "handle": "shopify",
        "class": "\\shopify\\Shopify"
    }
}
